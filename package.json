{
  "name": "graphapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsup",
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "codegen": "graphql-codegen",
    "apis": "git submodule update --recursive --remote && buf lint && buf generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bufbuild/buf": "^1.42.0",
    "@bufbuild/protobuf": "^1.0.0",
    "@bufbuild/protoc-gen-es": "^1.0.0",
    "@connectrpc/connect": "^1.0.0",
    "@connectrpc/connect-node": "^1.5.0",
    "@connectrpc/protoc-gen-connect-es": "^1.0.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.7.0",
    "pino": "^9.4.0"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.10.4",
    "@graphql-codegen/cli": "^5.0.2",
    "@types/node": "^22.5.5",
    "pino-pretty": "^11.2.2",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/*.graphql",
      "!src/**/*.graphqls"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}